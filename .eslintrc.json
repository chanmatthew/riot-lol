{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": [ "compat", "import", "promise"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "compat/compat": 2,
    "no-console": 1,
    "semi": 2,
    "no-unused-vars": 1
  },
  "globals": {
    "Promise": true
  },
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "settings": {
    "polyfills": ["fetch", "promises"]
  }
}